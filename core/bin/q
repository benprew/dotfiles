#!/usr/bin/env python3

import sys

# -s - sum
# -f or stdin
# -g - group by
# -o - order
# -d - delimiter (defaults to ' )

file = sys.argv[1]
sum_cols = sys.argv[2]
group_cols = sys.argv[3]

out = {}

with open(file, "r") as fh:
    for line in fh:
        row = line.split()
        k = []
        for g in map(int, group_cols.split(",")):
            k.append(row[g - 1])

        k = "|".join(k)

        for s in map(int, sum_cols.split(",")):
            if k not in out:
                out[k] = [0] * (len(row) - 1)

            if row[s - 1].isdigit():
                out[k][s - 1] += int(row[s - 1])

for k, v in out.items():
    print(k, " ".join(map(str, v)))
